<TabItem
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Mv.Modules.Schneider.Views"
         xmlns:pm="http://prismlibrary.com/" 
          xmlns:pg="http://propertytools.org/wpf"
            xmlns:ViewModels="clr-namespace:Mv.Modules.Schneider.ViewModels" x:Class="Mv.Modules.Schneider.Views.Settings"
         pm:ViewModelLocator.AutoWireViewModel="True"
             mc:Ignorable="d"
            Header="Schneider"
              d:DataContext="{d:DesignInstance ViewModels:SettingsViewModel}"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Margin="10">
        <pg:PropertyGrid VerticalAlignment="Top" SelectedObject="{Binding Config}" CategoryControlType="GroupBox"/>
    </Grid>
</TabItem>
