<TabItem x:Class="Mv.Modules.Axis.Views.Setting"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Mv.Modules.Axis.Views"
             xmlns:vm="clr-namespace:Mv.Modules.Axis.ViewModels"
             mc:Ignorable="d" 
            xmlns:prism="http://prismlibrary.com/"
             prism:ViewModelLocator.AutoWireViewModel="True"
            Header="Axis"
          xmlns:hc="https://handyorg.github.io/handycontrol" xmlns:pt="http://propertytools.org/wpf"
         d:DataContext="{d:DesignInstance vm:SettingViewModel}"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <pt:PropertyGrid SelectedObject="{Binding SelectedObject}"></pt:PropertyGrid>
    </Grid>

</TabItem>
