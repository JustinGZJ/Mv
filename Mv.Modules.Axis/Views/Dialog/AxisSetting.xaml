<UserControl x:Class="Mv.Modules.Axis.Views.Dialog.AxisSetting"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Mv.Modules.Axis.Views.Dialog"
             mc:Ignorable="d" 
             Style="{StaticResource MvDialogStyle}"
            xmlns:prism="http://prismlibrary.com/"
           prism:ViewModelLocator.AutoWireViewModel="True"
             xmlns:gd="http://propertytools.org/wpf"
             d:DesignHeight="450" d:DesignWidth="800">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="..\..\uiresources.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <gd:PropertyGrid SelectedObject="{Binding SelectedObject}"></gd:PropertyGrid>
    </Grid>
</UserControl>
