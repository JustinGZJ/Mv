<TabItem x:Class="Mv.Modules.Axis.Views.MainTab"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Mv.Modules.Axis.Views"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300"
             xmlns:prism="http://prismlibrary.com/"
            Header="轴控制"
             prism:ViewModelLocator.AutoWireViewModel="True" >
    <Grid>
        <TabControl>
            <TabItem Header="P2P">
                <local:Axis/>
            </TabItem>
            <TabItem Header="Follow">
                <local:FollowAxis></local:FollowAxis>
            </TabItem>
            <TabItem Header="IO">
                <local:InOut></local:InOut>
            </TabItem>
            <TabItem Header="Cylinders">
                <local:Cylinders></local:Cylinders>
            </TabItem>
            <TabItem Header="轴控制">
                <local:AxisesControl />
            </TabItem>
            <TabItem Header="气缸管理">
                <local:CylinderManager />
            </TabItem>
        </TabControl>
   
    </Grid>
</TabItem>
