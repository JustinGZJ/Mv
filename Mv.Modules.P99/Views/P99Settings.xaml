<TabItem x:Class="Mv.Modules.P99.Views.P99Settings"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Mv.Modules.P99.Views"
             mc:Ignorable="d" 
            xmlns:prism="http://prismlibrary.com/"
            prism:ViewModelLocator.AutoWireViewModel="True"
            Header="P99"
            xmlns:pg="http://propertytools.org/wpf"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid MinHeight="300">
        <DockPanel LastChildFill="True">
            <Button DockPanel.Dock="Bottom"  Command="{Binding SaveCommand}">保存</Button>
            <pg:PropertyGrid SelectedObject="{Binding Config}"></pg:PropertyGrid>
        </DockPanel>
    </Grid>
</TabItem>
